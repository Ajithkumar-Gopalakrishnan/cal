#include<stdio.h>
int main (){
char operator;
double n1,n2;
printf("enter an operator(+,-,*,/):");
scanf("%c",& operator);
printf("enter two operands;");
scanf("%\f%f",&n1,&n2);
Switch (operators)
{
Case'+';
printf("\\f+%\\f",n1,n2,n1+n2);
break;

case'-';
printf("%\\f-%\\f",n1,n2,n1-n2);
break;

case'*';
printf("%\\f*%\\f",n1,n2,n1*n2);

case'/';
printf("%\\f/%\\f",n1,n2,n1/n2);
break;
//operator doesn't match any case constant+,-,*,/,
default;
print("enter operator is not correct");
} return 0;
}
